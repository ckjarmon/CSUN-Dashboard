{
    "openapi": "3.0.0",
    "info": {
        "title": "CSUNDASH API",
        "description": "API for retrieving catalog information",
        "version": "1.0.0"
    },
    "paths": {
        "/{subject}/catalog": {
            "get": {
                "summary": "Get catalog information by subject",
                "parameters": [
                    {
                        "in": "path",
                        "name": "subject",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "The subject code"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "subject": {
                                                "type": "string"
                                            },
                                            "catalog_number": {
                                                "type": "string"
                                            },
                                            "title": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            },
                                            "units": {
                                                "type": "integer"
                                            },
                                            "prerequisites": {
                                                "type": "string"
                                            },
                                            "corequisites": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{subject}/professors": {
            "get": {
                "summary": "Get professors by subject",
                "parameters": [
                    {
                        "in": "path",
                        "name": "subject",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "The subject code"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "email": {
                                                "type": "string"
                                            },
                                            "first_name": {
                                                "type": "string"
                                            },
                                            "last_name": {
                                                "type": "string"
                                            },
                                            "image_link": {
                                                "type": "string"
                                            },
                                            "phone_number": {
                                                "type": "string"
                                            },
                                            "location": {
                                                "type": "string"
                                            },
                                            "website": {
                                                "type": "string"
                                            },
                                            "mail_drop": {
                                                "type": "string"
                                            },
                                            "subject": {
                                                "type": "string"
                                            },
                                            "office": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/<string:email>/rating": {
            "post": {
                "tags": [
                    "Ratings"
                ],
                "summary": "Post a new rating",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "email",
                        "type": "string",
                        "required": true,
                        "description": "Email address of the user submitting the rating"
                    },
                    {
                        "in": "body",
                        "name": "rating",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "professor_first_name": {
                                    "type": "string",
                                    "description": "First name of the professor"
                                },
                                "professor_last_name": {
                                    "type": "string",
                                    "description": "Last name of the professor"
                                },
                                "email": {
                                    "type": "string",
                                    "description": "Email address of the user submitting the rating"
                                },
                                "subject": {
                                    "type": "string",
                                    "description": "Subject of the course"
                                },
                                "catalog_number": {
                                    "type": "string",
                                    "description": "Catalog number of the course"
                                },
                                "star_rating": {
                                    "type": "integer",
                                    "description": "Star rating of the course"
                                },
                                "grade": {
                                    "type": "string",
                                    "description": "Grade received in the course"
                                },
                                "difficulty": {
                                    "type": "string",
                                    "description": "Difficulty level of the course"
                                },
                                "retake_professor": {
                                    "type": "boolean",
                                    "description": "Whether the user would retake the professor"
                                },
                                "require_textbooks": {
                                    "type": "boolean",
                                    "description": "Whether textbooks are required for the course"
                                },
                                "mandatory": {
                                    "type": "boolean",
                                    "description": "Whether the course is mandatory"
                                },
                                "review": {
                                    "type": "string",
                                    "description": "Review of the course"
                                },
                                "class_type": {
                                    "type": "string",
                                    "description": "Type of the course"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "professor_first_name": {
                                    "type": "string",
                                    "description": "First name of the professor"
                                },
                                "professor_last_name": {
                                    "type": "string",
                                    "description": "Last name of the professor"
                                },
                                "email": {
                                    "type": "string",
                                    "description": "Email address of the user submitting the rating"
                                },
                                "subject": {
                                    "type": "string",
                                    "description": "Subject of the course"
                                },
                                "catalog_number": {
                                    "type": "string",
                                    "description": "Catalog number of the course"
                                },
                                "star_rating": {
                                    "type": "integer",
                                    "description": "Star rating of the course"
                                },
                                "grade": {
                                    "type": "string",
                                    "description": "Grade received in the course"
                                },
                                "difficulty": {
                                    "type": "string",
                                    "description": "Difficulty level of the course"
                                },
                                "retake_professor": {
                                    "type": "boolean",
                                    "description": "Whether the user would retake the professor"
                                },
                                "require_textbooks": {
                                    "type": "boolean",
                                    "description": "Whether textbooks are required for the course"
                                },
                                "mandatory": {
                                    "type": "boolean",
                                    "description": "Whether the course is mandatory"
                                },
                                "review": {
                                    "type": "string",
                                    "description": "Review of the course"
                                },
                                "class_type": {
                                    "type": "string",
                                    "description": "Type of the course"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{email}/ratings": {
            "get": {
                "tags": [
                    "Ratings"
                ],
                "summary": "Get ratings by email",
                "description": "Retrieves ratings based on the provided email",
                "parameters": [
                    {
                        "name": "email",
                        "in": "path",
                        "description": "Email address of the user",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Rating"
                            }
                        }
                    }
                }
            }
        }
    },
